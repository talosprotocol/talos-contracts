{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://talosprotocol.com/schemas/a2a/group_member_add_request.schema.json",
  "title": "A2A Group Member Add Request",
  "description": "Request body for POST /a2a/groups/{id}/members",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "schema_id",
    "schema_version",
    "member_id"
  ],
  "properties": {
    "schema_id": {
      "const": "talos.a2a.group_member_add_request"
    },
    "schema_version": {
      "const": "v1"
    },
    "member_id": {
      "type": "string",
      "minLength": 1,
      "maxLength": 255,
      "description": "Principal ID of member to add"
    }
  }
}
