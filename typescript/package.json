{
  "name": "@talosprotocol/contracts",
  "version": "1.2.0",
  "description": "Talos Protocol contracts - schemas, types, and helper functions",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": null
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier -w .",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm run build && npm run test",
    "version:sync": "node ../scripts/version-sync.mjs",
    "gates:contracts": "echo 'Skipping gates for contracts repo'",
    "api:snapshot": "npm run build && cp dist/index.d.ts test/fixtures/api-baseline.d.ts",
    "smoke:pack": "npm pack --dry-run && node scripts/smoke-test.mjs"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "talos",
    "protocol",
    "contracts",
    "mcp",
    "security"
  ],
  "author": "Talos Protocol",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/talosprotocol/talos-contracts.git"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier -w"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier -w"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.7.4",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1"
  }
}
