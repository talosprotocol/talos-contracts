{
  "name": "@talosprotocol/contracts",
  "version": "1.0.7",
  "description": "Talos Protocol contracts - schemas, types, and helper functions",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier -w .",
    "typecheck": "tsc --noEmit",
    "version:sync": "node ./scripts/version-sync.mjs",
    "gates:contracts": "echo 'Skipping gates for contracts repo'"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "talos",
    "protocol",
    "contracts",
    "mcp",
    "security"
  ],
  "author": "Talos Protocol",
  "license": "MIT",
  "publishConfig": {
    "@talosprotocol:registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/talosprotocol/talos-contracts.git"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier -w"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier -w"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.7.4",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
